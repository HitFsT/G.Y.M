<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.dell.test">
    <!-- To auto-complete the email text field in the login form with the user's emails -->
    <uses-permission android:name="android.permission.GET_ACCOUNTS" />
    <uses-permission android:name="android.permission.READ_PROFILE" />
    <uses-permission android:name="android.permission.READ_CONTACTS" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="G.Y.M"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".StudentLoginActivity"
            android:label="Login" />
        <activity
            android:name=".StudentActivity"
            android:label="gym"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".StudentReservationActivity" />
        <activity android:name=".StudentTimeActivity" />
        <activity android:name=".StudentPlanActivity" />
        <activity android:name=".StaffLoginActivity" />
        <activity android:name=".StaffRegisterActivity" />
        <activity android:name=".StudentRegisterActivity" />
        <activity android:name=".StaffActivity" />
        <activity android:name=".MygymActivity"></activity>
    </application>

</manifest>